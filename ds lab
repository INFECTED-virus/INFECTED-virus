#include<stdio.h>
#include<stdlib.h>
int main()
{
    int i,n,key,a[n];
    printf("array elemetshow many ");
    scanf("%d",&n);
    printf("enter array elements");
    for(i=0;i<n;i++)
    scanf("%d",&a[i]);
    printf("enter the number you want to seacrch ");
    scanf("%d",&key);
    for(i=0;i<n;i++)
    if(key==a[i])
    {
        printf("key found%d",i+1);
        exit(0);
    
    }
    printf("element not found\t");
    return 0;
        /* code */
    
}
